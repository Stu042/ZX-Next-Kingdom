{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "dezog",
      "request": "launch",
      "name": "Z80 Debugger",
      "remoteType": "cspect",
      "rootFolder": "${workspaceFolder}",
      "z88dkv2": [
        {
          "path": "${workspaceFolder}/*.lis",
          "srcDirs": [],
          "mapFile": "${workspaceFolder}/kingdom.map",
          "excludeFiles": []
        }
      ],
      "disassemblerArgs": {
        "esxdosRst": true
      },
      "load": "${workspaceFolder}/kingdom.nex",
      "startAutomatically": true,
      "history": {
        "reverseDebugInstructionCount": 10000,
        "codeCoverageEnabled": false
      },
      "disassemblerArgs": {
                "numberOfLines": 20
            },
	                "smartDisassemblerArgs": {
                "lowerCase": false
            },
      "preLaunchTask": "Build",
      "topOfStack": "__register_sp"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/stu/speccy/code/c/CFrame-Layer2-Kingdom",
      "program": "/home/stu/speccy/code/c/CFrame-Layer2-Kingdom/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ],
  "compounds": []
}